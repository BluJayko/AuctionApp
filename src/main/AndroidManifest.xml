<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cool.AuctionApp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="AuctionApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.cool.auctionapp.LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.cool.auctionapp.HomeActivity" />
        <activity android:name="com.cool.auctionapp.SignUpActivity" />
        <activity
            android:name="com.cool.auctionapp.AddPostActivity"
            android:label="@string/title_activity_add_post" />
        <activity
            android:name="com.cool.auctionapp.MyProductsActivity"
            android:label="@string/title_activity_my_products" />
    </application>

</manifest>